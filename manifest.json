{
    "manifest_version": 3,
    "name": "VocabHub",
    "description": "Extension to improve reading comprehension and vocabulary",
    "version": "1.0",
    "action": {
      "default_popup": "home.html",
      "default_icon": "images/10558814.png"
    },
    "icons": {
      "16": "images/10558814.png"
    },
    "permissions": ["tabs"],
    "web_accessible_resources": [
      {
        "resources": ["popup.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }